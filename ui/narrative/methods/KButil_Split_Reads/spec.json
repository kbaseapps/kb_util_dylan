{
    "ver": "1.0.1",
    "authors": [
        "dylan"
    ],
    "contact": "http://kbase.us/contact-us/",
    "visible": true,
    "categories": ["active","util","reads"],
    "widgets": {
        "input": null,
        "output": "kbaseReportView"
    },
    "parameters": [ 
        {
            "id": "desc",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
		"is_output_name": false
            }
        },
        {
            "id": "input_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseFile.SingleEndLibrary","KBaseFile.PairedEndLibrary" ]
            }
        },
        {
            "id": "output_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseSets.ReadsSet" ],
		"is_output_name": true
            }
        },
        {
            "id": "split_num",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "10" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int"
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_util_dylan",
            "method": "KButil_Split_Reads",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "desc",
                    "target_property": "desc"
                },
                {
                    "input_parameter": "input_ref",
                    "target_property": "input_ref",
		    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                },
                {
                    "input_parameter": "split_num",
                    "target_property": "split_num"
                }
            ],
            "output_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
		    "constant_value": "5",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
